{
  "name": "np-platform-client",
  "version": "0.1.4",
  "license": "Apache-2.0",
  "scripts": {
    "demo": "expo start --web",
    "test": "jest",
    "coverage": "jest --coverage",
    "build": "babel . --out-dir dist --ignore node_modules,dist",
    "prepublishOnly": "yarn build"
  },
  "exports": {
    "./component/*": "./dist/component/*.js",
    "./feature": "./dist/feature/index.js",
    "./feature/*": "./dist/feature/*.js",
    "./organizer/*": "./dist/organizer/*.js",
    "./structure": "./dist/structure/index.js",
    "./structure/*": "./dist/structure/*.js",
    "./translations": "./dist/translations/index.js",
    "./util/*": "./dist/util/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wearenewpublic/np-platform"
  },
  "jest": {
    "preset": "jest-expo",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ]
  },
  "peerDependencies": {
    "expo": ">=48.0.1",
    "expo-font": "~11.1.1",
    "expo-linking": "~4.0.1",
    "expo-status-bar": "~1.4.4",
    "react": ">=16.13.1",
    "react-dom": ">=16.13.1",
    "react-native": ">=0.71.7",
    "react-native-web": ">=0.18.11"
  },
  "dependencies": {
    "firebase": "^9.22.2",
    "json5": "^2.2.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.22.4",
    "expo": ">=48.0.1",
    "expo-font": "~11.1.1",
    "expo-linking": "~4.0.1",
    "expo-status-bar": "~1.4.4",
    "react": ">=16.13.1",
    "react-dom": ">=16.13.1",
    "react-native": ">=0.71.7",
    "react-native-web": ">=0.18.11"
  }
}
